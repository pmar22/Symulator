<Window x:Class="Charts.LineChart"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        xmlns:local="clr-namespace:Charts"
        mc:Ignorable="d"
        Title="Wykres" Height="700" Width="1050">
    <Grid>
        <lvc:CartesianChart x:Name="chart" Series="{Binding SeriesCollection, NotifyOnSourceUpdated=True}" LegendLocation="Top" >
        </lvc:CartesianChart>
    </Grid>
</Window>
